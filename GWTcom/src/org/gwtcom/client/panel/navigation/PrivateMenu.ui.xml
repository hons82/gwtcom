  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:gwtcom='urn:import:org.gwtcom.client.panel'>

  <ui:style field='style'
    type='org.gwtcom.client.panel.navigation.PrivateMenu.Style'>

  .publicmenu {
    padding: 0.5em;
    line-height: 150%;
  }

  .item {
  	display: block;
 	}
